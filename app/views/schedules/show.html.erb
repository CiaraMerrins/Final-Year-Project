<p id="notice"><%= notice %></p>
<p>
  <strong>Section:</strong>
 
  <%= @schedule.section.name %>
</p>
<p>
  <strong>Description:</strong>
  <%= @schedule.description %>
</p>
<% if @schedule.image.present? %>
<p>
  <strong>Image:</strong><br>
 
  <%= image_tag @schedule.image.thumb.url, :height=>"225" %>
</p>
<% end %>
 <p><b>Coach:</b> <%= @schedule.coach.name %></p>
Submitted <%= time_ago_in_words(@schedule.created_at) %> Ago
<br>
<%= link_to 'Edit', edit_schedule_path(@schedule) %> |
<%= link_to 'Back', schedules_path %>
<br>
<%= link_to 'Delete', schedule_path(@schedule), method: :delete, data: {confirm: 'Are you sure?'} , class: "btn btn-default" %>
